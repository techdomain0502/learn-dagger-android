<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.android.learndagger">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:name="com.android.learndagger.MyApplication"
        android:label="@string/app_name"
        android:allowBackup="false"
        android:icon="@mipmap/ic_launcher"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:theme="@style/AppTheme">

        <activity android:name="com.android.learndagger.screens.questionslist.QuestionsListActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <activity android:name="com.android.learndagger.screens.questiondetails.QuestionDetailsActivity"/>

    </application>

</manifest>